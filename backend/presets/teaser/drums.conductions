instrument(drums) onBeat(1) {
    velocity += 190%;
}

instrument(drums) pitch(h) onBeat(1.5 2.5 3.5 4.5) {
    velocity = 70%;
}

instrument(drums) pitch(x) {
    velocity = & - 20;
}

instrument(drums) fromBar(4) pitch(x) onBeat(2 4) {
    velocity = 100;
}

instrument(drums) fromBar(4) pitch(x) onBeat(1 3) {
    velocity = 70;
}

instrument(drums) toBar(4) pitch(x) onBeat(1 2 3 4) {
    velocity = 70%;
}

instrument(drums) pitch(x) onBeat(1.5 2.5 3.5 4.5) {
    velocity = 100%;
}